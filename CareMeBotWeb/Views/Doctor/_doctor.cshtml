@model Data.ViewModels.DoctorSpecialityViewModel




<div class="panel-custom-default pl-10 pr-10">
    <div class="row">
        <div class="col-xs-12 p-15" style="background-color:#dcdcdc;border-top-left-radius:10px;border-top-right-radius:10px">
            <img class="center-block" style="border-radius:10px" src="@Model.doctor.PhotoUrl" width="100" height="100" />
        </div>
        <div class="clearfix"></div>
        <div class="col-xs-12 ">
            <h4 class="lh10 zawgyi_text pt-5"><i class="icon-person"></i>&ensp;@Model.doctor.Name_ZG</h4>
            @if (Model.specialty != null)
            {
                <small class="zawgyi_text text-muted lh10"><i class="icon-star-full2"></i> &ensp;@Model.specialty.Specialty_ZG</small>
            }
            else
            {
                <small class="zawgyi_text text-muted lh10"><i class="icon-star-full2"></i> &ensp;N/A</small>
            }
          
            <h6 class="" style="line-height:16px"><i class="icon-book"></i> &ensp;@Model.doctor.Qualification</h6>
        </div>
    </div>
</div>




